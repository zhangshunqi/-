#include<stdio.h>
int main()
{
    int a, b;
    int sum = 0;
    scanf("%d", &a);
    for(int i = 1;i < a;i++){
        if(a%i==0){
            sum += i;
        }
    }
    for(int j = 1;j <sum;j++){
        if(sum%j==0){
            b += j;
        }
    }
    if(b == a) printf("%d", sum);
    else printf("-1");
    return 0;
}
